main() {
	
	setlocale(LC_ALL,"Portuguese");
	
	float v1,v2,v3,v4,media;
	
	printf("digite o primeiro valor");
	scanf("%f",&v1); 
	
	printf("digite o Segundo valor");
	scanf("%f",&v2);
	
	printf("digite o terceiro valor");
	scanf("%f",&v3); 
	
	printf("digite o quarta valor");
	scanf("%f",&v4);
	
	media = (v1 + v2 + v3 + v4)/4;
	
	printf ("Nota 1 = %.2f \n Nota 2 = %.2f \n Nota 3 = %.2f \n Nota 4 = %.2f Media dos valores = %f    ",v1,v2,v3,v4,media);
  
  
  
