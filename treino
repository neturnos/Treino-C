main(){
	
	setlocale(LC_ALL,"Portuguese");
	int v1, v2;
	
	printf ("Digite o valor 1 = ");
	scanf ("%i",&v1);
	
	printf ("Digite o valor 2 = ");
	scanf ("%i",&v2);
	
	
	printf ("Resultado da soma  entre %i + %i = %i\n" ,v1,v2,v1+v2);
	printf ("Resultado da subitracao  entre %i - %i = %i\n" ,v1,v2,v1+-v2);
    printf ("Resultado da divisao  entre %i / %i = %i\n" ,v1,v2,v1/v2);
	printf ("Resultado da multiplicacao  entre %i * %i = %i\n" ,v1,v2,v1*v2);
	
	
	
}
